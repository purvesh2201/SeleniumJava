<?xml version="1.0" encoding="UTF-8"?>

<suite name="SeleniumJava" parallel="tests" thread-count="20">

	<listeners>
		<listener class-name="com.Automation.Listners.TestListener">
		</listener>
	</listeners>
		<test name="SeleniumJavaTest_edge">
		<parameter name="browser" value="edge" />
		<classes>
			<class name="com.Automation.Test.LoginPageTest"></class>
			<class name="com.Automation.Test.HomePageTest"></class>
		</classes>
	</test>

	<test name="SeleniumJavaTest_chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.Automation.Test.LoginPageTest"></class>
			<class name="com.Automation.Test.HomePageTest"></class>
		</classes>
	</test> 

	<test name="SeleniumJavaTest_firefox">
		<parameter name="browser" value="firefox" />
		<classes>
			<class name="com.Automation.Test.LoginPageTest"></class>
			<class name="com.Automation.Test.HomePageTest"></class>
		</classes>
	</test>

</suite>